<%= simple_form_for [forem, :admin, @forum] do |f| %>
  <div class= "input-big">
  <%= f.association :category, :include_blank => false %>
  <%= f.input :title%>
  <%= f.input :description, :as => :text %>
  </div>
  <h3>Moderator Groups</h3>
  <%= f.association :moderators, :as => :check_boxes, :class => 'btn btn-success' %>
  <%= f.submit %>
<% end %>
